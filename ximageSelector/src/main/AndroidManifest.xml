<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.move.ximageSelector">

    <!--拍照的权限-->
    <uses-permission android:name="android.permission.CAMERA" />

    <!-- 读取外部存储 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--创建文件-->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application android:allowBackup="true"
        android:supportsRtl="true">

        <activity android:name=".imageView.XSelectAct" android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".imageView.XPreviewAct"  android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

    </application>

</manifest>
